<?xml version="1.0"?>
<layout version="0.1.0">
    <default>
        <reference name="head">
            <action method="addJs">
                <script>tinybox/tinybox.js</script>
            </action>
            <action method="addCss">
                <stylesheet>css/tinybox/style.css</stylesheet>
            </action>
            <action method="addCss">
                <stylesheet>css/magestore/magenotification.css</stylesheet>
            </action>
        </reference>
    </default>	
    <!--        Changed By Adam 29/10/2015: Fix issue of SUPEE 6788 - in Magento 1.9.2.2-->
    <adminhtml_magenotification_feedback_index>
        <reference name="content">
            <block type="magenotification/adminhtml_feedback" name="extension_feedback" />
        </reference>
    </adminhtml_magenotification_feedback_index>

</layout>